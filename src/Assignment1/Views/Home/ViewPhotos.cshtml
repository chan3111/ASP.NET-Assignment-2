@model IEnumerable<Assignment2.Models.Photo>

@if (Model != null || Model.Count() > 0)
{
    <div class="photos">
        @foreach (var item in Model.Reverse())
        {
            <img src="@item.Url" /><a asp-controller="Home" asp-action="DeletePhoto" asp-route-id="@item.PhotoId" onclick="return confirm('Are you sure you want to delete this image?');">Delete</a>
        }
    </div>
}